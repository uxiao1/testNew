<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 制度MySQL驱动包的路径，别放在中文路径xia -->
    <classPathEntry location="C:\software\mavenRespository\mysql\mysql-connector-java\5.1.32\mysql-connector-java-5.1.32.jar"/>
    <context id="context1">
        <commentGenerator>
            <!--是否取消注销  -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://jsrl.ctitech.net:13306/DB_Heatmeter?characterEncoding=utf-8" userId="miniapp" password="ctitech0722">
        </jdbcConnection>
        <!-- 所生成的实体类的位置默认资源包src -->
        <javaModelGenerator targetPackage="com.zrdh.pojo" targetProject="src/main/java"></javaModelGenerator>
        <!--所生成的sqlMapper的映射文件位置，默认资源包src  -->
        <sqlMapGenerator targetPackage="com.zrdh.dao.lorawanUser" targetProject="src/main/resources"></sqlMapGenerator>
        <!--配置到接口生成信息  -->
        <javaClientGenerator targetPackage="com.zrdh.dao.lorawanUser" type="XMLMAPPER" targetProject="src/main/java"></javaClientGenerator>

        <!--为表生成代码tableName:表名,schema:不用填写  -->
        <table tableName="UM_UserLevel" enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="UM_UserData" enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="HM_OriginData" enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="HM_NormalDecodeData" enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="HM_LongFaultDecodeData" enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"></table>
    </context>
</generatorConfiguration>